                        <i class="searchicon fa fa-search"></i>
                        <div class="searchhelper">Search Reports, Charts, Emails or Notifications</div>
                    </div>
                    <!-- /Page Sidebar Header -->
                    <!-- Sidebar Menu -->
                    <ul class="nav sidebar-menu">
                        <li class="active">
                            <a href="/Admin/Home/Index"><i class="menu-icon glyphicon glyphicon-home"></i><span class="menu-text">Trang chủ</span></a>
                        </li>
                        <li>
                            <a class="menu-dropdown" href="#"><i class="menu-icon fa fa-user"></i><span class="menu-text">Hệ thống</span><i class="menu-expand"></i></a>    
                            <ul style="display: none;" class="submenu">
                                <li>
                                    <a href="/Admin/UserGroup/Index"><span class="menu-text">Nhóm người dùng</span></a>
                                </li>
                                <li>
